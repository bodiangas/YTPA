  "variables" : {
    "isValidFileWidgetValue" : {
      "type" : "expression",
      "value" : [
        "return angular.equals({}, $data.readExcelFile)",
        "    || $data.readExcelFile.contentType == \"application/vnd.openxmlformats-officedocument.spreadsheetml.sheet\";"
      ],
      "exposed" : false
    },
    "isFileValide" : {
      "type" : "expression",
      "value" : [
        "\r",
        " src='https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js';\r",
        " src='https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.8.0/jszip.js';\r",
        " src='https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.8.0/xlsx.js';\r",
        "   \r",
        "        var columnSpec = [\r",
        "            \"Assureur\",\r",
        "            \"Entite\",\r",
        "            \"Nom\",\r",
        "            \"Prenom\",\r",
        "            \"Genre\",\r",
        "            \"Filiation\",\r",
        "            \"NumPolice\",\r",
        "            \"NumReferent\",\r",
        "            \"NumCarte\",\r",
        "            \"RefCarte\",\r",
        "            \"StatutCard\",\r",
        "            \"DateNaissance\",\r",
        "            \"DebutConverture\",\r",
        "            \"FinCouverture\",\r",
        "            \"Non_Cov_Date\",\r",
        "            \"Langue\"\r",
        "        ];\r",
        "\r",
        "        var headerCells = [...Array(columnSpec.length)].map((_, index) => {\r",
        "            var headerCell = String.fromCharCode(index + 65) + '1';\r",
        "            var value = columnSpec[index];\r",
        "            return { key: headerCell, value: value }\r",
        "        });\r",
        "\r",
        "        let isWorkbookValid;\r",
        "\r",
        "            computeFileAsync(readExcelFile, function (result) {\r",
        "                isWorkbookValid = result;\r",
        "                return isWorkbookValid;\r",
        "\r",
        "            });\r",
        "\r",
        "\r",
        "\r",
        "        function computeFileAsync(file, callback) {\r",
        "            let fileReader = new FileReader();\r",
        "            fileReader.readAsBinaryString(file);\r",
        "            fileReader.onload = (event) => {\r",
        "                let data = event.target.result;\r",
        "                let workbook = XLSX.read(data, { type: \"binary\" });\r",
        "\r",
        "                callback(checkWorkbookValidity(workbook));\r",
        "            }\r",
        "        }\r",
        "\r",
        "        function checkWorkbookValidity(workbook) {\r",
        "            var workbookValidity;\r",
        "            workbook.SheetNames.forEach(sheet => {\r",
        "                workbookValidity = headerCells.every(headerCell => {\r",
        "                    let isheaderIncluded =\r",
        "                        !!workbook.Sheets[sheet][headerCell.key] &&\r",
        "                        workbook.Sheets[sheet][headerCell.key].v.toLowerCase() === headerCell.value.toLocaleLowerCase();\r",
        "\r",
        "                    if (!isheaderIncluded) return false;\r",
        "                    return true;\r",
        "                })\r",
        "            });\r",
        "            return workbookValidity;\r",
        "        }"
      ],
      "exposed" : false
    },
    "readExcelFile" : {
      "type" : "expression",
      "value" : [
        "return angular.continue($data.readExcelFile.content);"
      ],
      "exposed" : false
    }
  },
